{
	"name": "@pokedotdev/snowflake",
	"version": "0.0.3",
	"description": "High-performance TypeScript library for generating unique, sortable Snowflake IDs",
	"type": "module",
	"license": "MIT",
	"homepage": "https://github.com/pokedotdev/snowflake#readme",
	"bugs": {
		"url": "https://github.com/pokedotdev/snowflake/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pokedotdev/snowflake.git"
	},
	"author": "pokedotdev <git@poke.dev>",
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"test": "vitest",
		"typecheck": "tsc --noEmit",
		"release": "bumpp && npm publish"
	},
	"devDependencies": {
		"@types/node": "^22.15.17",
		"bumpp": "^10.1.0",
		"tsdown": "^0.11.9",
		"typescript": "^5.8.3",
		"vitest": "^3.1.3"
	}
}
